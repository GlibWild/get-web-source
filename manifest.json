{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "version": "1.0",
  "description": "__MSG_extensionDescription__",
  "default_locale": "zh_CN",
  "author": "GlibWild",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "html/result.html",
    "default_icon": {
      "16": "icons/lan.png",
      "48": "icons/lan.png",
      "128": "icons/lan128.png"
    }
  },
  "icons": {
    "16": "icons/lan.png",
    "48": "icons/lan.png",
    "128": "icons/lan128.png"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["js/content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "privacy_policy": "https://glibwild.github.io/get-web-source/privacy-policy-zh.html"
}
